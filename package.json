{
  "name": "@hiberbee/actions",
  "version": "1.0.0",
  "private": true,
  "description": "Github Actions monorepo",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "build:kops": "npm run clean && npm run format && npm run lint && npm run build && ncc build build/kops.js --source-map --out kops"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiberbee/actions.git"
  },
  "keywords": [
    "actions",
    "ci",
    "cd",
    "devops"
  ],
  "author": {
    "name": "Vlad Volkov",
    "url": "https://github.com/vladyslavvolkov",
    "email": "vlad@hiberbee.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/exec": "^1.0.4",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.0",
    "@actions/core": "^1.2.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "rimraf": "^3.0.2",
    "jest-circus": "^26.4.0",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}
